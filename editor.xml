<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_METHODS" value="true" />
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
    <option name="COLLAPSE_CUSTOM_FOLDING_REGIONS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="500" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="android.R" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="SF Mono" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Monaco" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="kotlin" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE:PREVIEW" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseAccessors" value="true" />
    <option name="collapseAnnotations" value="true" />
    <option name="collapseAnonymousClasses" value="true" />
    <option name="collapseEndOfLineComments" value="true" />
    <option name="collapseInnerClasses" value="true" />
    <option name="collapseMultilineComments" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="OCCodeFoldingSettings">
    <option name="collapseBlockExpressions" value="true" />
    <option name="collapseMultilineComments" value="true" />
    <option name="COLLAPSE_MULTILINE_COMMENTS" value="true" />
    <option name="COLLAPSE_BLOCK_EXPRESSIONS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bbacklog\b.*" />
  </component>
</application>